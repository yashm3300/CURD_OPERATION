@model List<CategoryMaster.Models.Domain.Category>
@{
}
<h1>Category Details</h1>
<table class="table">
    <thead>
        <tr>
            <th>CategoryID</th>
            <th>CategoryName</th>
            <th>Description</th>
            <th> </th>




        </tr>
    </thead>
    <tbody>

        @foreach (var category in Model)
        {
        <td>@category.CategoryId</td>
        <td>@category.CategoryName</td>
        <td>@category.Description</td>
        <td><a href="Categorys/View/@category.CategoryId">View</a></td>
        }
    </tbody>
</table>